<StartScreen>:
    rows: 1
    canvas.before:
        Rectangle:
            pos: self.pos
            size: self.size
            source: 'StartScreen.png'

    FloatLayout:
        BoxLayout:
            size_hint: (0.75, 0.4)
            pos_hint: {'center_x':0.5, 'center_y':0.4}
            orientation: 'vertical'

            MDFillRoundFlatIconButton:
                text: 'Log in'
                size_hint: (1, 0.2)
                icon: "login-variant"
                on_press: root.pressed_log_in()

            MDLabel:
                size_hint: (1, 0.05)

            MDFillRoundFlatIconButton:
                text: 'Sign up'
                size_hint: (1, 0.2)
                icon: "login-variant"
                on_press: root.pressed_sign_up()

            MDLabel:
                size_hint: (1, 0.05)

            MDFillRoundFlatIconButton:
                text: 'Instructions'
                size_hint: (1, 0.2)
                icon: "menu"
                on_press: root.pressed_instru()

            MDLabel:
                size_hint: (1, 0.05)

            MDFillRoundFlatIconButton:
                text: 'Exit'
                size_hint: (1, 0.2)
                icon: "logout-variant"
                on_press: root.pressed_exit()


<LogInScreen>:

    username: username

    FloatLayout:
        MDLabel:
            text:"[b]Log in[/b]"
            markup: True
            font_style: "H3"
            pos_hint: {"center_x": 0.5, "center_y": 0.75}
            halign: 'center'
            valign: 'middle'

        MDLabel:
            text:"Welcome back :)"
            font_style: "Subtitle1"
            theme_text_color: "Secondary"
            pos_hint: {"center_x": 0.5, "center_y": 0.67}
            halign: 'center'
            valign: 'middle'

        MDTextFieldRound:
            id: username
            icon_left: "account"
            hint_text: "Enter username (no spaces!)"
            multiline: False
            size_hint: (0.75, 0.05)
            pos_hint: {"center_x": 0.5, "center_y": 0.55}

        MDFillRoundFlatButton:
            text: "Submit"
            size_hint: (0.45, 0.075)
            pos_hint: {"center_x": 0.5, "center_y": 0.45}
            on_press: root.pressed()

        MDLabel:
            id: error_msg
            text: ""
            text_size: root.width, None
            size: self.texture_size
            pos_hint: {"center_x": 0.5, "center_y": 0.37}
            halign: 'center'
            valign: 'middle'


